# EditorConfig is awesome: https://EditorConfig.org

# Top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

# Data and Schema files
[*.{xml,xsd}]
charset = utf-8-bom
end_of_line = crlf
indent_size = 2
