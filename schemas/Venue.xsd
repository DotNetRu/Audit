<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="Types.xsd"/>

  <xs:element name="Venue">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Id" type="identity" />
        <xs:element name="Name" type="requaredString" />
        <xs:element name="Capacity" type="xs:positiveInteger" minOccurs="0" maxOccurs="1" />
        <xs:element name="Address" type="requaredString" />
        <xs:element name="MapUrl" type="requaredUri" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
